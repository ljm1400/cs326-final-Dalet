{
  "name": "cs326-final-dalet",
  "version": "1.0.0",
  "description": "An application for users to share outdoor activity picutres and experiences.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongodb": "^3.6.3",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^7.13.0"
  },
  "engines": {
    "node": "13.x"
  },
  "scripts": {
    "start": "node server.js",
    "lint": "eslint client/scripts server.js db.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "consistent-return": "error",
        "no-var": "error",
        "eqeqeq": "error",
        "semi": [
            "error",
            "always"
        ],
        "curly": [
            "warn",
            "all"
        ],
        "prefer-const": [
            "warn",
            {}
        ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ljm1400/cs326-final-Dalet.git"
  },
  "author": "Team-Dalet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ljm1400/cs326-final-Dalet/issues"
  },
  "homepage": "https://github.com/ljm1400/cs326-final-Dalet#readme"
}
